print_info() {
    info title
    info underline

    info "OS" distro
    info "Kernel" kernel
    info "Uptime" uptime
    info "Packages" packages
    info "Shell" shell
    info "Resolution" resolution
    info "Terminal" term
    info "CPU" cpu
    info "GPU Driver" gpu_driver
    info "Battery" battery
    info "Memory" memory

    info line_break
    info cols
    info line_break
}

