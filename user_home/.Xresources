! Term name
URxvt*termName: screen-256color
URxvt*loginShell: true

URxvt*scrollTtyOutput: false
URxvt*scrollWithBuffer: true
URxvt*scrollTtyKeypress: true

! Clickable URLs
URxvt.perl-ext-common: default,matcher
URxvt.url-launcher: /usr/bin/xdg-open
URxvt.matcher.button: 1

! Cursor
Xcursor.theme: Pulse-Glass-Sapphire-Regular
Xcursor.size: 16

! Font
URxvt*font: xft:DejaVu Sans Mono:style=Oblique:size=12

! Disable scrollbar
URxvt*scrollBar: false
URxvt*scrollBar_right: false
URxvt*scrollBar_floating: false

! Modifier
URxvt*modifier: alt

! Theme
urxvt*foreground: #f2f2f2
urxvt*background: #101010
urxvt*color0:     #101010
urxvt*color1:     #f13a21
urxvt*color2:     #93f91d
urxvt*color3:     #ffd00a
urxvt*color4:     #004f9e
urxvt*color5:     #ec0048
urxvt*color6:     #2aa7e7
urxvt*color7:     #f2f2f2
urxvt*color8:     #1d202f
urxvt*color9:     #ff361e
urxvt*color10:    #ffc005
urxvt*color11:    #93ff00
urxvt*color12:    #0071ff
urxvt*color13:    #ef0051
urxvt*color14:    #4bb8fd
urxvt*color15:    #a020f0
